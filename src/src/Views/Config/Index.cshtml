@model src.Models.Organization
@inject UserManager<ApplicationUser> UserManager
@{
    ViewData["Title"] = "Members";

    Layout = "_LayoutAdminlteAdmin";

}

<!-- Content Header (Page header) -->
<!-- Main content -->

<section class="content">
    <div class="row">
        <div class="col-lg-12">
            <div class="box box-primary">
                <div class="box-body">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="col-md-6">
                                <a class="btn btn-success" style="margin-bottom:10px" onclick="ShowPopup('@Url.Action("AddEditIndex","Members")')"><i class="fa fa-plus"></i> New Member</a>
                            </div>
                            @*<input type="hidden" asp-for="@Model.organizationId" />*@
                            <table id="grid" name="grid" class="table table-striped table-bordered">
                                <thead>
                                    <tr>
                                        <th class="col-md-1">Date Started</th>
                                        <th class="col-md-1">Name</th>
                                        <th class="col-md-1">Date Expiration</th>
                                        @*<th class="col-md-1">Age</th>*@
                                        <th class="col-md-1">Locker Number</th>
                                        <th class="col-md-1">Status</th>
                                        <th class="col-md-1">Remarks</th>
                                        <th class="col-md-1">Actions</th>
                                    </tr>
                                </thead>
                            </table>
                        </div>
                    </div>

                </div>

            </div>
        </div>

    </div>

</section>


<!-- /.content -->
<!--modal placeholder-->
<div class="modal modal-primary fade" id="modalDefault">
    <div class="modal-dialog">
        <div class="modal-content">

        </div>
    </div>
</div>



@section Scripts{
    @await Html.PartialAsync("_ValidationScriptsPartial")


    <script src="~/js/views/members/index.js"></script>




}

