@model src.Models.Organization
@inject UserManager<ApplicationUser> UserManager
@{
    ViewData["Title"] = "Monthly Collection";

    Layout = "_LayoutAdminlteAdmin";

}

<!-- Content Header (Page header) -->
<!-- Main content -->

<section class="content">
    <div class="row">
        <div class="col-lg-12">
            <div class="box box-primary">
                <div class="box-body">
                    <div class="row">
                        <div class="col-lg-12">

                            @*<input type="hidden" asp-for="@Model.organizationId" />*@
                            <table id="grid" name="grid" class="table table-striped table-bordered">
                                <thead>
                                    <tr>
                                        <th class="col-md-1">Date</th>
                                        <th class="col-md-1">Origin</th>
                                        <th class="col-md-1">Amount</th>
                                        <th class="col-md-1">Remarks</th>
                                        <th class="col-md-1">Total</th>
                                    </tr>
                                </thead>
                            </table>
                        </div>
                    </div>

                </div>

            </div>
        </div>

    </div>

</section>


<!-- /.content -->
<!--modal placeholder-->
<div class="modal modal-primary fade" id="modalDefault">
    <div class="modal-dialog">
        <div class="modal-content">

        </div>
    </div>
</div>



@section Scripts{
    @await Html.PartialAsync("_ValidationScriptsPartial")


    <script src="~/js/views/monthlycollection/index.js"></script>




}

